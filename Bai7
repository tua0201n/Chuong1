using System;

class Program
{
    static void Main()
    {
        Console.Write("Nhap so nguyen 4 byte: ");
        int n_138 = int.Parse(Console.ReadLine());

        if (n_138 > 0 && (n_138 & (n_138 - 1)) == 0)
        {
            Console.WriteLine($"{n_138} la so co dang 2^k.");
        }
        else
        {
            Console.WriteLine($"{n_138} khong phai la so co dang 2^k.");
        }
    }
}

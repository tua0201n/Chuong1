using System;

class Program
{
     static int TinhTong(int n)
      {
          return (n * (n + 1)) / 2;
      }
     static void Main()
     {
         Console.Write("Nhap so nguyen n: ");
         int n = int.Parse(Console.ReadLine());

         int sum_138 = TinhTong(n);
         Console.WriteLine($"Tong S({n}) = {sum}");
     }
}

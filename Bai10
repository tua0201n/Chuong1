using System;

class Program
{
    static double TinhLuyThua(double x, int n)
    {
        double ketQua = 1;
        for (int i = 0; i < n; i++)
        {
            ketQua *= x;
        }
        return ketQua;
    }
    static void Main()
    {
        Console.Write("Nhap co so x: ");
        double x = double.Parse(Console.ReadLine());

        Console.Write("Nhap so mu n: ");
        int n = int.Parse(Console.ReadLine());

        double result = TinhLuyThua(x, n);
        Console.WriteLine($"{x}^{n} = {result}");
    }

}
